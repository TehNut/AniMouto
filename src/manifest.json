{
  "name": "AniMouto",
  "short_name": "AniMouto",
  "author": "Nicholas \"TehNut\" Ignoffo",
  "description": "A little sister to enhance your AniList experience.",
  "key": "",
  "offline_enabled": false,
  "permissions": [
    "storage",
    "alarms",
    "identity",
    "*://*.anilist.co/*"
  ],
  "optional_permissions": [
    "notifications"
  ],
  "background": {
    "scripts": [
      "backend/anilist_util.js",
      "backend/alarms.js",
      "backend/oauth_handler.js",
      "backend/update_listener.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "assets/img/animouto_16x.png",
      "32": "assets/img/animouto_32x.png",
      "48": "assets/img/animouto_48x.png",
      "128": "assets/img/animouto_128x.png"
    }
  },
  "icons": {
    "16": "assets/img/animouto_16x.png",
    "32": "assets/img/animouto_32x.png",
    "48": "assets/img/animouto_48x.png",
    "128": "assets/img/animouto_128x.png"
  },
  "web_accessible_resources": [
    "/oauth2/*"
  ],
  "manifest_version": 2
}
